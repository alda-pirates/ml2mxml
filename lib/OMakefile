.SUBDIRS:

OCAMLPACKS += camlp4
OCAMLFINDFLAGS += -syntax camlp4o
OCAML_LINK_FLAGS += -pack music_xml,music

OCAMLPACKS[] =
    cow
    cow.syntax

FILES[] =
    music_xml
    drum_music_xml
    music

.PHONY: clean

